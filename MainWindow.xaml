<Window x:Class="INotifyPropertyChangedInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:INotifyPropertyChangedInterface"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Label Content="First Name" Canvas.Left="103" Canvas.Top="68" />
        <TextBox Width="250" Height="20" Canvas.Left="209" Canvas.Top="68" Text="{Binding Path=FName}"/>
        
        <Label Content="Last Name" Canvas.Left="100" Canvas.Top="107" />
        <TextBox Width="250" Height="20" Canvas.Left="209" Canvas.Top="113" Text="{Binding Path=LName}" />
        
        <Label Content="Full Name" Canvas.Left="103" Canvas.Top="152" />
        <TextBox Width="250" Height="20" Canvas.Left="209" Canvas.Top="152" Text="{Binding Path=FullName}"/>
    </Canvas>
</Window>
